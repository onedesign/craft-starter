{
  "name": "onedesign/craft-starter",
  "description": "Craft CMS",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "project"
  ],
  "license": "Unlicense",
  "homepage": "https://craftcms.com/",
  "type": "project",
  "support": {
    "email": "support@craftcms.com",
    "issues": "https://github.com/craftcms/cms/issues",
    "forum": "https://craftcms.stackexchange.com/",
    "source": "https://github.com/craftcms/cms",
    "docs": "https://craftcms.com/docs",
    "rss": "https://craftcms.com/changelog.rss"
  },
  "require": {
    "craftcms/cms": "^4.2.0.2",
    "vlucas/phpdotenv": "^5.4.0",
    "clubstudioltd/craft-asset-rev": "^7.0",
    "craftcms/aws-s3": "^2.0",
    "craftcms/ckeditor": "^2.0",
    "nystudio107/craft-autocomplete": "^1.0",
    "nystudio107/craft-seomatic": "^4.0",
    "nystudio107/craft-typogrify": "^4.0",
    "onedesign/atomic-deploy": "^1.1",
    "putyourlightson/craft-blitz": "^4.0",
    "topshelfcraft/environment-label": "^4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@composer dump-autoload -o"
    ]
  }
}
