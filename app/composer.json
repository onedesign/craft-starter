{
  "require": {
    "craftcms/ckeditor": "^4.1.0",
    "craftcms/cms": "^5.6.11",
    "nystudio107/craft-seomatic": "^5.0.4",
    "nystudio107/craft-typogrify": "^5.0.1",
    "servd/craft-asset-storage": "^4.0.3",
    "spacecatninja/imager-x": "^5.0.1",
    "verbb/formie": "^3.0.0-beta.16",
    "verbb/hyper": "^2.0.2",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "nystudio107/craft-autocomplete": "^1.11",
    "roave/security-advisories": "dev-latest",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
